<resilience4j-test>
    <io>
        <github>
            <resilience4j>
                <circuitbreaker>
                    <default>
                        <automaticTransitionFromOpenToHalfOpenEnabled>true</automaticTransitionFromOpenToHalfOpenEnabled>
                        <failureRateThreshold>1.0</failureRateThreshold>
                        <minimumNumberOfCalls>1</minimumNumberOfCalls>
                        <permittedNumberOfCallsInHalfOpenState>1</permittedNumberOfCallsInHalfOpenState>
                        <slidingWindowSize>1</slidingWindowSize>
                        <slidingWindowType>TIME_BASED</slidingWindowType>
                        <slowCallDurationThreshold>PT1S</slowCallDurationThreshold>
                        <slowCallRateThreshold>1</slowCallRateThreshold>
                        <waitDurationInOpenState>PT1S</waitDurationInOpenState>
                        <writableStackTraceEnabled>true</writableStackTraceEnabled>
                        <ignoreExceptions>java.io.IOException</ignoreExceptions>
                        <recordExceptions>java.lang.Exception</recordExceptions>
                    </default>
                    <autoTransitionFalse>
                        <automaticTransitionFromOpenToHalfOpenEnabled>false</automaticTransitionFromOpenToHalfOpenEnabled>
                    </autoTransitionFalse>
                    <failureRate>
                        <failureRateThreshold>3.0</failureRateThreshold>
                    </failureRate>
                    <minimumCalls>
                        <minimumNumberOfCalls>4</minimumNumberOfCalls>
                    </minimumCalls>
                    <numberInHalfOpen>
                        <permittedNumberOfCallsInHalfOpenState>5</permittedNumberOfCallsInHalfOpenState>
                    </numberInHalfOpen>
                    <slidingWindowSize>
                        <slidingWindowSize>6</slidingWindowSize>
                    </slidingWindowSize>
                    <countBasedWindow>
                        <slidingWindowType>COUNT_BASED</slidingWindowType>
                    </countBasedWindow>
                    <slowCallDuration>
                        <slowCallDurationThreshold>PT8S</slowCallDurationThreshold>
                    </slowCallDuration>
                    <slowCallRateThreshold>
                        <slowCallRateThreshold>9</slowCallRateThreshold>
                    </slowCallRateThreshold>
                    <waitDurationOpen>
                        <waitDurationInOpenState>PT10S</waitDurationInOpenState>
                    </waitDurationOpen>
                    <writableStack>
                        <writableStackTraceEnabled>false</writableStackTraceEnabled>
                    </writableStack>
                    <ignore>
                        <ignoreExceptions>java.lang.RuntimeException</ignoreExceptions>
                    </ignore>
                    <record>
                        <recordExceptions>java.lang.RuntimeException</recordExceptions>
                    </record>
                </circuitbreaker>
            </resilience4j>
        </github>
    </io>
    <custom>
        <noDefault>
            <circuitbreaker>
                <autoTransitionTrue>
                    <automaticTransitionFromOpenToHalfOpenEnabled>true</automaticTransitionFromOpenToHalfOpenEnabled>
                </autoTransitionTrue>
                <failureRate>
                    <failureRateThreshold>13.0</failureRateThreshold>
                </failureRate>
                <minimumCalls>
                    <minimumNumberOfCalls>14</minimumNumberOfCalls>
                </minimumCalls>
                <numberInHalfOpen>
                    <permittedNumberOfCallsInHalfOpenState>15</permittedNumberOfCallsInHalfOpenState>
                </numberInHalfOpen>
                <slidingWindowSize>
                    <slidingWindowSize>16</slidingWindowSize>
                </slidingWindowSize>
                <timeBasedWindow>
                    <slidingWindowType>TIME_BASED</slidingWindowType>
                </timeBasedWindow>
                <slowCallDuration>
                    <slowCallDurationThreshold>PT18S</slowCallDurationThreshold>
                </slowCallDuration>
                <slowCallRateThreshold>
                    <slowCallRateThreshold>19</slowCallRateThreshold>
                </slowCallRateThreshold>
                <waitDurationOpen>
                    <waitDurationInOpenState>PT20S</waitDurationInOpenState>
                </waitDurationOpen>
                <writableStack>
                    <writableStackTraceEnabled>true</writableStackTraceEnabled>
                </writableStack>
                <ignore>
                    <ignoreExceptions>java.lang.RuntimeException</ignoreExceptions>
                </ignore>
                <record>
                    <recordExceptions>java.lang.RuntimeException</recordExceptions>
                </record>
            </circuitbreaker>
        </noDefault>
    </custom>
</resilience4j-test>